              function setBreadcrumDiv() {
                  var breadcrumbDiv = document.getElementById('breadcrumb-div');
                  breadcrumbDiv.innerHTML = '<table cellspacing="0"><tr>' +
                   '<td class="breadcrumb-link"><a href="../index.jag">QA Home</a>' +
