                 <div id="middle">

                  <%
var req_data = request.getParameter("data");
json_data = parse(decodeURIComponent(req_data));
if(json_data.list.WSO2_QAP_TEST_CASE_ID===null || json_data.list.WSO2_QAP_TEST_CASE_NAME===null){
    print("Sorry this page isn't available");
}else{
    print("<h2>");
    print(json_data.list.WSO2_QAP_TEST_CASE_NAME+" test case");
    print("</h2>");
}
%>


<div id="workArea">

   <form  name="productsForm" method="post">
      <input name="pageNumber" value="0" type="hidden">
      <input name="webappState" value="all" type="hidden">
      <input name="webappType" value="all" type="hidden">
      <table class="styledLeft" id="webappTable" style="margin-left: 0px;"
                           width="100 ">

         <thead>
          <tr class="tableEvenRow">
            <th colspan="2" align="left"><strong><%= json_data.list.WSO2_QAP_TEST_CASE_NAME%></strong></th>

          </tr>
          </thead>

           <tbody>
           <tr class="tableOddRow">
           <td>
                <strong>Description</strong>
           </td>
            <td>
            <%= json_data.list.WSO2_QAP_DESCRIPTION%>
            </td>
           </tr>

            <tr class="tableEvenRow">
           <td>
                <strong>Automation Status</strong>
           </td>
            <td>
            <%= json_data.list.WSO2_QAP_AUTOMATION_STATUS%>
            </td>
